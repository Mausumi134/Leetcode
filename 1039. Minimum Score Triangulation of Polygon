class Solution {
public:
    int mini(vector<int>& v,int i,int j,vector<vector<int>>& dp){
        if(j-i<=1)return 0;
        if(j-i==2)return v[i]*v[i+1]*v[j];
        if(dp[i][j]!=-1)return dp[i][j];
        int res=INT_MAX;
        for(int k=i+1;k<=j-1;k++){
           res = min(res, mini(v, i, k, dp) + v[i] * v[j] * v[k] + mini(v, k, j, dp));

        }
        dp[i][j]=res;
        return res;
    }
    int minScoreTriangulation(vector<int>& values) {
        int n=values.size();
        vector<vector<int>>dp(n,vector<int>(n,-1));
        return mini(values,0,n-1,dp);
    }
};